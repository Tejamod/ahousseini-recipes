<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Description</key>
    <string>Downloads the latest version of 3CXPhone and creates a package.</string>
    <key>Identifier</key>
    <string>com.github.ahousseini-recipes.pkg.3CX</string>
    <key>Input</key>
    <dict>
      <key>NAME</key>
      <string>3CX</string>
      <key>APPNAME</key>
      <string>3CXPhone.app</string>
      <key>PKG_ID</key>
      <string>com.3cx.3cxphone</string>
    </dict>
    <key>MinimumVersion</key>
    <string>1.0.0</string>
    <key>ParentRecipe</key>
    <string>com.github.michalmmac.download.3CX</string>
    <key>Process</key>
    <array>
      <dict>
        <key>Processor</key>
        <string>Copier</string>
        <key>Arguments</key>
        <dict>
          <key>source_path</key>
          <string>%pathname%/3CXPhone.app</string>
          <key>destination_path</key>
          <string>%RECIPE_CACHE_DIR%/%NAME%/3CXPhone.app</string>
          <key>overwrite</key>
          <true/>
        </dict>
      </dict>
      <dict>
        <key>Processor</key>
        <string>AppPkgCreator</string>
        <key>Arguments</key>
        <dict>
          <key>app_path</key>
          <string>%RECIPE_CACHE_DIR%/%NAME%/3CXPhone.app</string>
        </dict>
      </dict>
    </array>
  </dict>
</plist>
